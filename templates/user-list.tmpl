<h3>Users</h3>
<div class="list-group">
  {{#each users}}
    <div userID="{{this._id}}" class="userItem list-group-item list-group-item-info {{this.active}}">
      {{#if this.loggedIn}}
        [ONLINE]
      {{/if}}

      {{this.username}}
      {{#if this.active}}
      <button
        id="logoutButton"
        class="logoutButton pull-right btn btn-xs btn-warning"
        type="button"
      >logout</button>
      {{/if}}
    </div>
  {{/each}}
</div>
